/* pages/casedetail/casedetail.wxss */

/* 在使用的Wxss中引入WxParse.css */
@import "../../utils/wxParse/wxParse.wxss";

page {
  width: 100%;
  height: 100%;
}
.case-detail {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 20rpx;
}

.case-detail .case-detail-title {
  text-align: center;
  display: block;
  font-size: 34rpx;
  color: #666666;
  line-height: 1.5;
}

.case-detail .case-detail-time {
  text-align: center;
  display: block;
  font-size: 24rpx;
  color: #666666;
  line-height: 2.5;
}

.case-detail .case-detail-content {
  font-size: 30rpx;
  color: #666666;
  text-align: justify;
}
